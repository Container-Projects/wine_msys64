[fedora-updates]
name=Fedora - x86_64 - Updates
failovermethod=priority
#baseurl=http://archive.fedoraproject.org/pub/archive/fedora/linux/updates/19/x86_64/
metalink=https://mirrors.fedoraproject.org/metalink?repo=updates-released-f19&arch=x86_64
enabled=0
gpgcheck=1
metadata_expire=6h
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-x86_64
skip_if_unavailable=False
